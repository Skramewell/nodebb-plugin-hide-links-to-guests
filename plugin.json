{
  "id": "nodebb-plugin-hide-links-to-guests",
  "name": "Hide links to guests",
  "description": "Hide links to guest for NodeBB",
  "url": "https://github.com/Skramewell/nodebb-plugin-hide-links-to-guests",
  "library": "./library.js",
  "languages": "languages",
  "defaultLang": "en-GB",
  "hooks": [
    { "hook": "static:app.load", "method": "init" },
    { "hook": "filter:post.getPosts", "method": "alterContent" },
    { "hook": "filter:admin.header.build", "method": "addAdminNavigation" }
  ],
  "templates": "static/templates",
  "modules":{"../admin/plugins/hide-content.js":"./static/lib/hide-content.js"}
  
}
